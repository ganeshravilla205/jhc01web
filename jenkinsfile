pipeline{
    agent any
    stages{
        stage("maven build"){
            steps{
                sh 'mvn clean package'
            }
        }
        stage("deploy to develop"){
        when{
        branch="develop"
            steps{
                echo "deploying to develop branch"
            }
        }
        }
          stage("deploy to stage"){
          when{
          branch="stage"
            steps{
                echo "deploying to stage branch"
            }
        }
        }
        stage("deploy to prod"){
        when{
        branch="prod"
            steps{
                echo "deploying to prod branch"
            }
        }
    }
}
